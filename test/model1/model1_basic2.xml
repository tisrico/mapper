                </tag>
              </match-criteria>
            </flexible-match>
            <ingress-rewrite>
              <push-tag xmlns="urn:bbf:yang:bbf-sub-interface-tagging">
                <index>1</index>
                <dot1q-tag>
                  <tag-type xmlns:bbf-dot1qt="urn:bbf:yang:bbf-dot1q-types">bbf-dot1qt:c-vlan</tag-type>
                  <vlan-id>2781</vlan-id>
                  <pbit-from-tag-index>1</pbit-from-tag-index>
                  <dei-from-tag-index>1</dei-from-tag-index>
                </dot1q-tag>
              </push-tag>
              <push-tag xmlns="urn:bbf:yang:bbf-sub-interface-tagging">
                <index>0</index>
                <dot1q-tag>
                  <tag-type xmlns:bbf-dot1qt="urn:bbf:yang:bbf-dot1q-types">bbf-dot1qt:c-vlan</tag-type>
                  <vlan-id>vlan-id-from-tag-index</vlan-id>
                  <vlan-id-from-tag-index-or-discard>1</vlan-id-from-tag-index-or-discard>
                  <pbit-from-tag-index>1</pbit-from-tag-index>
                  <dei-from-tag-index>1</dei-from-tag-index>
                </dot1q-tag>
              </push-tag>
            </ingress-rewrite>
          </rule>
        </ingress-rule>
        <egress-rewrite>
          <push-tag xmlns="urn:bbf:yang:bbf-sub-interface-tagging">
            <index>0</index>
            <dot1q-tag>
              <tag-type xmlns:bbf-dot1qt="urn:bbf:yang:bbf-dot1q-types">bbf-dot1qt:c-vlan</tag-type>
